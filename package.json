{
  "name": "ease-web",
  "version": "1.0.0",
  "description": "Web client for ease video player",
  "main": "app.js",
  "scripts": {
    "setup": "npm install && npm prune && typings install && npm run rebuild",
    "start": "node ./dist/app.js",
    "clean": "rimraf dist/*",
    "rebuild": "npm run clean && npm run build",
    "build": "npm run build:wp",
    "build:wp": "webpack",
    "watch": "concurrently --kill-others \"npm run watch:wp\" \"npm run watch:ts\"",
    "watch:wp": "webpack -w",
    "watch:ts": "tsc -w",
    "test": "echo TODO",
    "lint": "npm run lint:src",
    "lint:src": "tslint \"src/**/*.ts?(x)\" --project ./tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Right2Drive/ease-web.git"
  },
  "pre-commit": [
    "lint",
    "rebuild"
  ],
  "keywords": [
    "ease",
    "video-streaming",
    "p2p",
    "react",
    "webpack",
    "typescript"
  ],
  "author": "Right2Drive",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Right2Drive/ease-web/issues"
  },
  "homepage": "https://github.com/Right2Drive/ease-web#readme",
  "dependencies": {
    "express": "^4.15.2",
    "guid": "0.0.12",
    "material-design-lite": "^1.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-mdl": "^1.9.0",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "simple-peer": "^7.0.0",
    "socket.io-client": "^1.7.3",
    "style-loader": "^0.14.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.8",
    "@types/react": "^15.0.16",
    "@types/react-dom": "^0.14.23",
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.27.3",
    "file-loader": "^0.10.1",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.0",
    "tslint": "^4.5.1",
    "tslint-react": "^2.5.0",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  }
}
