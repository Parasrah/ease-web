{
  "name": "ease-web",
  "version": "1.0.0",
  "description": "Web client for ease video player",
  "main": "app.js",
  "scripts": {
    "setup": "npm install && npm prune && npm run rebuild",
    "start": "node ./app.js",
    "start:watch": "concurrently --kill-others \"npm run start\" \"npm run watch\"",
    "clean": "rimraf dist/*",
    "rebuild": "npm run clean && npm run build",
    "build": "npm run build:wp && npm run build:ts",
    "build:wp": "webpack",
    "build:ts": "tsc --diagnostics --project .",
    "watch": "concurrently \"npm run watch:wp\" \"npm run watch:ts\"",
    "watch:wp": "webpack -w",
    "watch:ts": "tsc -w",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "tslint \"src/**/*.ts?(x)\" --project ./tslint.json",
    "lint:src-fix": "npm run lint:src -- --fix",
    "lint:test": "tslint \"test/**/*.ts?(x)\" --project ./tslint.json",
    "lint:src-test": "npm run lint:test -- --fix",
    "test": "npm run test:unit && npm run test:unit:server",
    "test:unit": "npm run test:unit:client && npm run test:unit:server",
    "test:unit:client": "npm run test:unit:client:communications && npm run test:unit:client:peer && npm run test:unit:client:reducers && echo TODO_ENABLE_VIEW_TESTS",
    "test:unit:client:communications": "echo NO_COMMS_TESTS",
    "test:unit:client:peer": "mocha --recursive dist/test/unit/client/peer",
    "test:unit:client:peer-debug": "npm run test:unit:client:peer -- --debug",
    "test:unit:client:reducers": "echo NO_REDUCER_TEST",
    "test:unit:client:view": "mocha --recursive dist/test/unit/client/view --opts ./test/support/mocha.opts",
    "test:unit:client:view-debug": "npm run test:unit:client:view -- --debug",
    "test:unit:server": "echo NO_SERVER_TESTS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Right2Drive/ease-web.git"
  },
  "pre-commit": [
    "lint",
    "rebuild"
  ],
  "keywords": [
    "ease",
    "video-streaming",
    "p2p",
    "react",
    "webpack",
    "typescript"
  ],
  "author": "Right2Drive",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Right2Drive/ease-web/issues"
  },
  "homepage": "https://github.com/Right2Drive/ease-web#readme",
  "dependencies": {
    "cfenv": "^1.0.4",
    "express": "^4.15.2",
    "guid": "0.0.12",
    "material-design-lite": "^1.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-mdl": "^1.9.0",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.10.2",
    "redux-thunk": "^2.2.0",
    "simple-peer": "^7.0.0",
    "socket.io-client": "^1.7.3",
    "url-loader": "^0.5.8"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/enzyme": "^2.7.6",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "@types/react": "^15.0.16",
    "@types/react-dom": "^0.14.23",
    "@types/react-mdl": "^1.7.20",
    "@types/react-redux": "^4.4.37",
    "@types/redux-logger": "^2.10.0",
    "@types/simple-peer": "^6.1.0",
    "@types/sinon": "^1.16.36",
    "@types/socket.io-client": "^1.4.29",
    "@types/webpack": "^2.2.11",
    "awesome-typescript-loader": "^3.1.2",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.7.1",
    "file-loader": "^0.10.1",
    "jsdom": "^9.12.0",
    "jsdom-global": "^2.1.1",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "mocha": "^3.2.0",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.14.1",
    "testdouble": "^2.0.3",
    "tslint": "^4.5.1",
    "tslint-react": "^2.5.0",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  }
}
